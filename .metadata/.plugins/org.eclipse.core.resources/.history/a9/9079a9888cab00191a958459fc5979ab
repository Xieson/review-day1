package com.chlp.team;
/*
 * 知识:数组
 * 
 * 
 * */
public class Arraydome {
	public static void main(String[] args){
		int[] arr=new int[]{21,5,65,87,45,242,1,1,6548,76987,434,23,6565,67};
		System.out.println("数组的索引长度为:"+arr.length);
		System.out.println("数组中索引是4位置的值是:"+arr[4]);
		System.out.println("数组的最大值为"+arr);
	}
}
